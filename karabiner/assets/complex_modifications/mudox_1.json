{ "title": "Mudox #2",
  "rules": [
    {
      "description": "option + f/b/a/e move arrows word-wise",
      "manipulators": [{
        "conditions": [{
          "bundle_identifiers": [
            "^com\\.apple\\.Terminal$",
            "^com\\.googlecode\\.iterm2$",
            "^co\\.zeit\\.hyperterm$",
            "^co\\.zeit\\.hyper$"
          ],
          "type": "frontmost_application_unless"
        }],
        "from": {
          "key_code": "a",
          "modifiers": {
            "mandatory": [
              "right_option"
            ]
          }
        },
        "to": [{
          "key_code": "left_arrow",
          "modifiers": [
            "left_command"
          ]
        }],
        "type": "basic"
      }, {
      "conditions": [{
        "bundle_identifiers": [
          "^com\\.apple\\.Terminal$",
          "^com\\.googlecode\\.iterm2$",
          "^co\\.zeit\\.hyperterm$",
          "^co\\.zeit\\.hyper$"
        ],
        "type": "frontmost_application_unless"
      }],
      "from": {
        "key_code": "e",
        "modifiers": {
          "mandatory": [
            "right_option"
          ]
        }
      },
      "to": [{
        "key_code": "right_arrow",
        "modifiers": [
          "left_command"
        ]
      }],
      "type": "basic"
      }, {
      "conditions": [{
        "bundle_identifiers": [
          "^com\\.apple\\.Terminal$",
          "^com\\.googlecode\\.iterm2$",
          "^co\\.zeit\\.hyperterm$",
          "^co\\.zeit\\.hyper$"
        ],
        "type": "frontmost_application_unless"
      }],
      "from": {
        "key_code": "f",
        "modifiers": {
          "mandatory": [
            "right_option"
          ]
        }
      },
      "to": [{
        "key_code": "right_arrow",
        "modifiers": [
          "right_option"
        ]
      }],
      "type": "basic"
      }, {
      "conditions": [{
        "bundle_identifiers": [
          "^com\\.apple\\.Terminal$",
          "^com\\.googlecode\\.iterm2$",
          "^co\\.zeit\\.hyperterm$",
          "^co\\.zeit\\.hyper$"
        ],
        "type": "frontmost_application_unless"
      }],
      "from": {
        "key_code": "b",
        "modifiers": {
          "mandatory": [
            "right_option"
          ]
        }
      },
      "to": [{
        "key_code": "left_arrow",
        "modifiers": [
          "right_option"
        ]
      }],
      "type": "basic"
      }]
    }
  ]}
