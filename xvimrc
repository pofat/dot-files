set ignorecase
set wrapscan

set guioptions=

nnoremap ]] /^[-+]|func\b<Cr>zz
nnoremap [[ ?^[-+]|func\b<Cr>zz

nnoremap n nzz
nnoremap N Nzz

nnoremap <C-h> <C-w>h
nnoremap <C-j> <C-w>j
nnoremap <C-k> <C-w>k
nnoremap <C-l> <C-w>l
noremap J     8j
noremap K     8k
noremap H     <C-u>
noremap L     <C-d>

inoremap <C-H> <Left>
inoremap <C-L> <Right>

nnoremap zh    :xhelp<CR>
nnoremap ]q    :ni<CR>
nnoremap [q    :pi<CR>

inoremap <C-h> <left>
inoremap <C-j> <down>
inoremap <C-k> <up>
inoremap <C-l> <right>
inoremap <C-a> <C-o>^
inoremap <C-e> <C-o>$

nnoremap [<Space> O<Esc>j
nnoremap ]<Space> o<Esc>k
nnoremap ]e ddp
nnoremap [e ddkP


